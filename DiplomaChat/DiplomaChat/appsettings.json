{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "TimeStamp: {Timestamp:HH:mm:ss.fff zzz} MachineName: {MachineName} {CorrelationId}{RequestMethod}{RequestPath}{StatusCode}{Elapsed}{UserId}{RequestBody}{ResponseBody}{Message}{NewLine}{Exception}"
        }
      }
    ],
    "MinimumLevel": "Warning",
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*",
  "JwtConfiguration": {
    "Issuer": "DiplomaChat",
    "Audience": "DiplomaChat",
    "TokenLifetime": 7,
    "SecretKey": "B?E(H+MbQeThWmZq4t7w9z$C&F)J@NcR",
    "ValidateLifetime": "False",
    "RequireExpirationTime": "False"
  },
  "HashConfiguration": {
    "Salt": "TestSalt"
  },
  "ConnectionStrings": {
    "DIPLOMA_CHAT_CORE_DB_CONNECTION_STRING": "Data Source=localhost;Initial Catalog=DiplomaChat;User ID=SA;Password=yourStrong(!)Password"
  },
  "RequestLimitConfiguration": {
    "MaxRequestLimit": 20,
    "MinRequestLimit": 1,
    "Default": 10
  },
  "RabbitMQCOnfiguration": {
    "HostName": "localhost",
    "Port": 15672,
    "VirtualHost": "",
    "UserName": "guest",
    "Password": "guest"
  }
}
