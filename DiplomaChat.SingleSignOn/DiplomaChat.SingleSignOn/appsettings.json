{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "TimeStamp: {Timestamp:HH:mm:ss.fff zzz} MachineName: {MachineName} {CorrelationId}{RequestMethod}{RequestPath}{StatusCode}{Elapsed}{UserId}{RequestBody}{ResponseBody}{Message}{NewLine}{Exception}"
        }
      }
    ],
    "MinimumLevel": "Warning",
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*",
  "JwtConfiguration": {
    "Issuer": "DiplomaChat-SingleSignOn",
    "Audience": "DiplomaChat-SingleSignOn",
    "TokenLifetime": 7,
    "SecretKey": "9z$C&F)J@NCRfTjWnZr4u7x!A%D*G-Ua",
    "ValidateLifetime": false,
    "RequireExpirationTime": false
  },
  "HashConfiguration": {
    "Salt": "TestSalt"
  },
  "Urls": "https://[:]:5005;http://[:]:5004",
  "ConnectionStrings": {
    "DIPLOMA_CHAT_SSO_DB_CONNECTION_STRING": "Data Source=(LocalDB)\\MSSQLLocalDB;Initial Catalog=DiplomaChat;Integrated Security=True;"
  }
}
